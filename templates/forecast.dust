{>"page"/}

{<bodyContent}
<section class="body row">
    <div class="wrapper col-xs-12">
        {#forecast}
            <h1 class="subject">{subject}</h1>
            <p class="text">{body}</p>
            <div class="media">
                {?media}
                    <img class="img forecast-img" src="{media}" alt="{media}">
                {/media}
            </div>
        {/forecast}
    </div>
</section>
{/bodyContent}